{
  "name": "app-backend",
  "version": "1.0.0",
  "description": "Everything related to backend will be done here",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "test:unit": "jest --coverage --config jest.config.js",
    "test:integration": "dotenv -e .env.test -- jest --coverage --config jest.integration.config.js --runInBand",
    "test:security": "dotenv -e .env.security -- jest --config jest.security.config.js --forceExit",
    "test:security:jwt": "dotenv -e .env.security -- jest tests/security/jwt-protection.security.test.ts --config jest.security.config.js --forceExit",
    "test:security:passwords": "dotenv -e .env.security -- jest tests/security/password-security.security.test.ts --config jest.security.config.js --forceExit",
    "test:security:headers": "dotenv -e .env.security -- jest tests/security/security-headers.security.test.ts --config jest.security.config.js --forceExit",
    "test:security:https": "dotenv -e .env.security -- jest tests/security/https-security.security.test.ts --config jest.security.config.js --forceExit",
    "test:nonfunctional": "npm run test:security"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.18.10",
    "@types/supertest": "^6.0.3",
    "@types/tinycolor2": "^1.4.6",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.16.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.911.0",
    "@aws-sdk/client-s3": "^3.890.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@prisma/client": "^6.16.2",
    "@types/node-fetch": "^2.6.13",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "form-data": "^4.0.3",
    "google-search-results-nodejs": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "node-fetch": "^2.7.0",
    "tinycolor2": "^1.6.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  }
